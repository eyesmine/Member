{{>layout/header}}

    <h1>회원 관리 웹 서비스</h1>
    <div class="col-md-12">
        <div class="row">
            <div class="col-md-6">
                <a href="/members/save" role="button" class="btn btn-primary">회원 등록</a>
            </div>
        </div>
    </div>

    <!-- 목록 출력 영역 -->
    <table class="table table-horizontal table-bordered">
        <thead class="thead-strong">
        <tr>
            <th>회원번호</th>
            <th>ID</th>
            <th>PW</th>
            <th>최종수정일</th>
        </tr>
        </thead>
        <tbody id="tbody">
        {{#lists}}
            <tr>
                <td>{{id}}</td>
                <td>{{userId}}</td>
                <td><a href="/members/update/{{id}}">{{pw}}</a></td>
                <td>{{modifiedDate}}</td>
            </tr>
        {{/lists}}
        </tbody>
    </table>
    </div>

{{>layout/footer}}
